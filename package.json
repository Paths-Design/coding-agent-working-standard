{
  "name": "caws",
  "version": "1.0.0",
  "description": "CAWS - Coding Agent Workflow System (Turborepo)",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "turbo run format",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "validate": "turbo run validate",
    "clean": "turbo run clean",
    "cli": "cd packages/caws-cli && npm run start",
    "scaffold": "cd packages/caws-cli && npm run start scaffold",
    "init": "cd packages/caws-cli && npm run start init"
  },
  "keywords": ["caws", "cli", "scaffolding", "agents", "workflow", "turborepo", "monorepo"],
  "author": "@darianrosebrook",
  "license": "MIT",
  "packageManager": "npm@10.0.0",
  "devDependencies": {
    "turbo": "^2.0.0",
    "prettier": "^3.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "babel-jest": "^29.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=10.0.0"
  }
}
