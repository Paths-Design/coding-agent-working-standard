{
  "name": "@caws/template",
  "version": "1.0.0",
  "description": "CAWS (Coding Agent Workflow System) project template",
  "scripts": {
    "build": "echo 'Template package uses JavaScript - no TypeScript compilation needed' && exit 0",
    "dev": "tsc --watch",
    "test": "echo 'Template package has no tests - this is expected'",
    "test:unit": "echo 'Template package has no unit tests - this is expected'",
    "test:contract": "echo '⚠️  Contract tests not configured'",
    "test:integration": "echo '⚠️  Integration tests not configured'",
    "test:e2e:smoke": "echo '⚠️  E2E smoke tests not configured'",
    "test:mutation": "echo '⚠️  Mutation tests not configured'",
    "test:axe": "echo '⚠️  Accessibility tests not configured'",
    "test:watch": "jest --watch",
    "lint": "eslint apps/tools/**/*.js",
    "lint:fix": "eslint apps/tools/**/*.js --fix",
    "lint:staged": "eslint apps/tools/**/*.js",
    "format": "prettier --write apps/tools/**/*.js .caws/**/*.json .caws/**/*.yaml",
    "validate": "node apps/tools/caws/validate.js .caws/working-spec.yaml",
    "caws:validate": "node ../../.caws/validate.js ../../.caws/working-spec.yaml",
    "clean": "rm -rf dist .agent"
  },
  "keywords": [
    "caws",
    "template",
    "scaffolding",
    "agents",
    "workflow",
    "turborepo"
  ],
  "author": "Paths Design <hello@paths.design>",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "12.0.2",
    "@types/node": "^20.0.0",
    "ajv": "^8.12.0",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-node": "^11.1.0",
    "jest": "30.1.3",
    "js-yaml": "4.1.0",
    "json-schema": "^0.4.0",
    "lint-staged": "15.5.2",
    "prettier": "^3.0.0",
    "semantic-release": "25.0.0-beta.6",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
