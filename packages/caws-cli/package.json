{
  "name": "@caws/cli",
  "version": "1.0.0",
  "description": "CLI tool for scaffolding CAWS (Coding Agent Workflow System) projects",
  "main": "dist/index.js",
  "bin": {
    "caws": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js tests/**/*.js",
    "lint:fix": "eslint src/**/*.js tests/**/*.js --fix",
    "format": "prettier --write src/**/*.js tests/**/*.js",
    "clean": "rm -rf dist test-caws-project .agent"
  },
  "keywords": [
    "caws",
    "cli",
    "scaffolding",
    "agents",
    "workflow",
    "turborepo"
  ],
  "author": "@darianrosebrook",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.12.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "fs-extra": "^11.0.0",
    "inquirer": "^9.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/js-yaml": "^4.0.0",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.0.0",
    "js-yaml": "4.1.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
